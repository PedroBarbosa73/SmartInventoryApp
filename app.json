{
  "expo": {
    "name": "SmartInventory",
    "slug": "SmartInventory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you select images for your inventory items.",
          "cameraPermission": "The app accesses your camera to let you take photos of your inventory items."
        }
      ]
    ],
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5DC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nandexx98.SmartInventory",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#F5F5DC"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.nandexx98.SmartInventory"
    },
    "extra": {
      "eas": {
        "projectId": "6284bc6c-d555-4efc-bb02-f038a0befd2a"
      }
    }
  }
}
